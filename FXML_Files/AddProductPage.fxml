<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="co" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddProductController">
   <children>
      <Label alignment="CENTER" layoutX="217.0" layoutY="52.0" prefHeight="40.0" prefWidth="164.0" text="Product Values" textAlignment="CENTER">
         <font>
            <Font name="Times New Roman" size="24.0" />
         </font>
      </Label>
      <JFXTextField fx:id="addNameValue" layoutX="25.0" layoutY="175.0" prefHeight="25.0" prefWidth="60.0" promptText="Name" />
      <JFXTextField fx:id="addHeightValue" layoutX="125.0" layoutY="175.0" prefHeight="25.0" prefWidth="60.0" promptText="Height" />
      <JFXTextField fx:id="addWidthValue" layoutX="225.0" layoutY="175.0" prefHeight="25.0" prefWidth="60.0" promptText="Width" />
      <JFXTextField fx:id="addWeightValue" layoutX="325.0" layoutY="175.0" prefHeight="25.0" prefWidth="60.0" promptText="Weight" />
      <JFXTextField fx:id="addPcsValue" layoutX="425.0" layoutY="175.0" prefHeight="25.0" prefWidth="60.0" promptText="Pcs" />
      <JFXTextField fx:id="addPriceValue" layoutX="525.0" layoutY="175.0" prefHeight="25.0" prefWidth="60.0" promptText="Price" />
      <JFXButton fx:id="goBackButton" buttonType="RAISED" layoutX="230.0" layoutY="329.0" onAction="#goBackButtonEvent" prefHeight="40.0" prefWidth="140.0" text="Go Back">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="saveButton" layoutX="249.0" layoutY="237.0" onAction="#saveButtonEvent" prefHeight="35.0" prefWidth="100.0" text="Save">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="clearButton" buttonType="RAISED" layoutX="460.0" layoutY="239.0" onAction="#clearButtonEvent" text="Clear">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
